pipeline{
    agent any
	stages{
	    stage("gitcheckout"){
		steps{
		    echo "helloworld"
		     }
	    }
	    stage("second step"){
		steps{
		    echo "f**k off !21!!!"
		     }
	    }
	    stage("cleanup"){
		    steps{
		        echo "cleaning up working space!!"
			deleteDir()   
		    }
	    }
	}

}
